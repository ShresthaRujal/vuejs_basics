<!--Note that these codes are tested on online edit Called JSFIDDLE --> https://jsfiddle.net/fhe0dwvm/51/-->
<!-- Html--> 
<script src="https://unpkg.com/vue/dist/vue.js"></script>

<div id="app">
  <p v-if="show"> 
  you can see me!
  </p>
  <p v-else>
  Now you see me!
  </p>
  <p>
  Do you also see me?
  </p>
  <button v-on:click="show = !show">Switch
  </button>
</div>

<script>
new Vue({
	el: '#app',
 	data:{
  	show:true,
  }
});
</script>